{
    "description": "Run text_corrector script with environment",
    "manipulators": [
        {
            "from": {
                "key_code": "q",
                "modifiers": { "mandatory": ["left_control"] }
            },
            "to": [{ "shell_command": "~/Scripts/correcter/correcter_launcher.sh" }],
            "type": "basic"
        },
        {
            "from": {
                "key_code": "t",
                "modifiers": { "mandatory": ["left_control"] }
            },
            "to": [{ "shell_command": "~/Scripts/correcter/correcter_launcher.sh --rephrase" }],
            "type": "basic"
        }
    ]
}
